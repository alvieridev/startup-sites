{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "StartUp Sites",
      "permissions": ["tabs", "storage", "activeTab", "windows"],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "32": "icon.png"
    },
      "background": {
    "service_worker": "src/background/service-worker.ts"
  },
    "description":  "Automatically open your favorite sites when browser starts",
    "action": {
        "default_popup": "index.html"
    }
}